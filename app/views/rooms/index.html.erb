<div class="container">
  <div class="row no-gutters">
    <div class="col-9" style="padding: 10px 10px 10px 10px;">
      <strong><p>All Rooms</p></strong>
      <div class="card-deck">
        <%= render "room", {rooms: @rooms} %>
      </div>
    </div>

    <div class="col-3" style="padding-left: 10px;">
      <strong><p>Top</p></strong>
      <%= render "top", {rooms: @rooms} %>
    </div>
  </div>
</div>
