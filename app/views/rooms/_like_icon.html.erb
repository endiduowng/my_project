<% if is_liked.present? %>
  <%= link_to like_path(is_liked), method: :delete, remote: true do %>
      <%= image_tag "liked.png", width: "48px", height: "48px" %>
  <% end %>
<% else %>
  <%= link_to room_likes_path(room), method: :post, remote: true do %>
      <%= image_tag "like.png", width: "48px", height: "48px" %>
  <% end %>
<% end %>
